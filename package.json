  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "lint": "jshint --config=.jshintrc --reporter=node_modules/jshint-stylish file.js",
    "test": "npm run lint",
    "clean": "rimraf dist/* docs/*",
    "prebuild": "npm run clean",
    "build:css": "lessc less/main.less | autoprefixer -b 'last 1 version' | cssmin > dist/css/main.css",
    "build:js": "cp -r app/* dist/",
    "cp": "cp -r *.html *.txt *.ico fonts/ img/ scripts/ dist/",
    "serve": "lite-server --baseDir './dist' --files 'dist/**/*'"
  },
