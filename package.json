{
  "name": "tradity-client",
  "version": "0.7.0",
  "devDependencies": {
    "angular-gettext-cli": ">= 1.0.7 < 2.0.0",
    "angular-write-constants": "^1.0.1",
    "autoprefixer": "latest",
    "browser-sync": "latest",
    "connect-history-api-fallback": "latest",
    "cssmin": "latest",
    "grunt": "~0.4.1",
    "grunt-angular-gettext": "~2.1.3",
    "grunt-angular-templates": "~0.5.4",
    "grunt-autoprefixer": "~0.4.0",
    "grunt-bower-install": "~0.7.0",
    "grunt-cli": "~0.1.13",
    "grunt-concurrent": "~0.4.1",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-coffee": "~0.7.0",
    "grunt-contrib-compass": "~0.6.0",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-connect": "~0.5.0",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-cssmin": "~0.7.0",
    "grunt-contrib-htmlmin": "~0.1.3",
    "grunt-contrib-imagemin": "~0.3.0",
    "grunt-contrib-jshint": "~0.7.1",
    "grunt-contrib-less": "^0.11.0",
    "grunt-contrib-uglify": "~0.2.0",
    "grunt-contrib-watch": "~0.5.2",
    "grunt-google-cdn": "~0.2.0",
    "grunt-karma": "~0.6.2",
    "grunt-newer": "~0.5.4",
    "grunt-ngmin": "~0.0.2",
    "grunt-rev": "~0.1.0",
    "grunt-svgmin": "~0.2.0",
    "grunt-usemin": "~2.0.0",
    "hashmark": "latest",
    "jshint": "^2.8.0",
    "jshint-stylish": "~0.1.3",
    "karma": "~0.10.9",
    "karma-chrome-launcher": "~0.1.2",
    "karma-coffee-preprocessor": "~0.1.3",
    "karma-firefox-launcher": "~0.1.3",
    "karma-html2js-preprocessor": "~0.1.0",
    "karma-jasmine": "~0.1.5",
    "karma-ng-html2js-preprocessor": "~0.1.0",
    "karma-ng-scenario": "~0.1.0",
    "karma-phantomjs-launcher": "~0.1.2",
    "karma-requirejs": "~0.2.1",
    "karma-script-launcher": "~0.1.0",
    "less": "latest",
    "levenshtein": "~1.0",
    "load-grunt-tasks": "~0.2.0",
    "munkres-js": "*",
    "ngdoc-md": "^1.0.3",
    "parallelshell": "latest",
    "postcss-cli": "latest",
    "remarkup-po": "latest",
    "requirejs": "~2.1.11",
    "rimraf": "latest",
    "time-grunt": "~0.2.1",
    "watch-run": "latest"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "scripts": {
    "lint": "jshint --config=.jshintrc --reporter=node_modules/jshint-stylish app/**/*.js",
    "test": "npm run lint",
    "gettext-extract": "angular-gettext-cli --files '{app/**/*.html,app/js/**/*.js}' --dest po/unstripped/templates.pot",
    "gettext-compile": "angular-gettext-cli --compile --files po/unstripped/de.po --dest dist/app/js/jit/l10n/de.json --module tradity --format json",
    "unmarkup": "remarkup-po unmarkup po/unstripped/templates.pot > po/templates.pot",
    "remarkup": "remarkup-po remarkup po/unstripped/templates.pot po/de.po > po/unstripped/de.po",
    "translate-extract": "npm run gettext-extract && npm run unmarkup",
    "translate-build": "npm run remarkup && npm run gettext-compile",
    "translate-cycle": "npm run translate-extract && npm run translate-build",
    "clean": "rimraf dist/ docs/ && mkdir -p docs/ dist/app/js/jit/l10n/",
    "docs": "ngdoc-md 'app/js/**/*.js' docs/",
    "prebuild": "npm run clean",
    "build": "parallelshell 'npm run build:css' 'npm run build:js' 'npm run build:cp' 'npm run translate-cycle'",
    "build:css": "lessc less/main.less | postcss --use autoprefixer --autoprefixer.browsers 'last 1 version' | cssmin > dist/main.css",
    "build:js": "parallelshell 'npm run build:js:copy' 'npm run build:js:stamp' 'npm run build:js:config'",
    "build:js:copy": "cp -r app/ dist/",
    "build:js:config": "touch app/js/config.local.json && angular-write-constants --module tradity app/js/config.global.json app/js/config.local.json > dist/app/js/config.js",
    "build:js:stamp": "echo \"var TRADITY_BUILD_STAMP = 'TDYC$(date +%s)-$(git rev-parse HEAD)';\" > dist/app/js/buildstamp.js",
    "build:cp": "cp -r *.html *.txt *.ico fonts/ img/ scripts/ dist/",
    "watch": "parallelshell 'npm run watch:css' 'npm run watch:js' 'npm run watch:cp'",
    "watch:css": "watch-run -p less/ npm run build:css",
    "watch:js": "watch-run -p app/ npm run build:js",
    "watch:cp": "watch-run -p '*.html, *.txt, *.ico, fonts/, img/, scripts/' npm run build:cp",
    "start": "npm run build && parallelshell 'npm run watch' 'browser-sync start --config bs-config.js'"
  },
  "dependencies": {
    "angular": "1.5.0-rc.0",
    "angular-debounce": "~1.0.1",
    "angular-dialog-service": "m-e-conroy/angular-dialog-service#b6471bb",
    "angular-events-calendar": "^1.0.0",
    "angular-gettext": "~2.1.2",
    "angular-md5": "*",
    "angular-sanitize": "1.5.0-rc.0",
    "angular-ui-bootstrap": "0.14.x",
    "angular-ui-router": "~0.2.14",
    "asmcrypto": "vibornoff/asmcrypto.js#v0.0.6",
    "bootstrap": "3.3.x",
    "clipboard": "~1.5.3",
    "connect-modrewrite": "^0.7.5",
    "cytoscape": "~2.5.0",
    "debug": "nhaesler/debug#100cf7e",
    "font-awesome": "4.5.0",
    "grunt-connect-rewrite": "^0.2.1",
    "grunt-ngdocs": "^0.2.6",
    "jquery": "2.1.1",
    "lzma": "addaleax/LZMA-JS#development-all",
    "munkres-js": ">=1.1.1",
    "ng-infinite-scroll": "1.2.x",
    "nsPopover": "nohros/nsPopover#622a133b344b",
    "open-sans-fontface": "~1.2.0",
    "socket.io-client": ">=1.2.1",
    "tradity-connection": "^0.10.0",
    "zxcvbn": "~4.0.1"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tradity/tradity-client.git"
  },
  "bugs": {
    "url": "https://github.com/tradity/tradity-client/issues"
  }
}
